{
    "name": "Call Me Maybe",
    "manifest_version": 2,
    "version": "0.1",
    "description": "Run process on page activated by click in extension popup",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js","socket.io.js","contentscript.js","popupwindow.js"]
        // dont remove the socket import from the above line 
    },
    "permissions": [
        "tabs"
    ]
}