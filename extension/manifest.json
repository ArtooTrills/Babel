{
    "name": "Call Me Maybe",
    "manifest_version": 2,
    "version": "0.1",
    "description": "Run process on page activated by click in extension popup",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["/socket.io.js", "background.js"]
    },
    "permissions": [
        "tabs", "http://*/*", "https://*/*","https://cdnjs.cloudflare.com"
    ]
}